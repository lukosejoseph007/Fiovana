# .env.development
# Development Environment Configuration for Proxemic

# Environment
PROXEMIC_ENV=development
RUST_LOG=debug

# Security
PROXEMIC_DEBUG=true
PROXEMIC_MAX_FILE_SIZE=104857600
PROXEMIC_ENCRYPTION_KEY=dev_secure_32_character_key_here___

# AI Service Configuration (encrypted values recommended)
OPENROUTER_API_KEY=your_dev_openrouter_api_key
ANTHROPIC_API_KEY=your_dev_anthropic_api_key

# Database Configuration
DATABASE_URL=sqlite:./Proxemic_dev.db

# Vector Search Configuration
VECTOR_INDEX_PATH=./vector_index_dev
MAX_EMBEDDING_BATCH_SIZE=100

# Feature Flags
ENABLE_TELEMETRY=true
ENABLE_CLOUD_SYNC=true
ENABLE_COLLABORATION=true
