{
  "app": {
    "name": "Proxemic",
    "version": "0.1.0",
    "environment": "Production",
    "debug": false,
    "max_file_handles": 50,
    "request_timeout_ms": 30000,
    "max_concurrent_operations": 5
  },
  "security": {
    "allowed_extensions": [".docx", ".pdf", ".md", ".txt"],
    "max_path_length": 260,
    "max_file_size": 52428800,
    "allowed_mime_types": [
      "application/pdf",
      "text/plain",
      "text/markdown",
      "application/vnd.openxmlformats-officedocument.wordprocessingml.document"
    ],
    "prohibited_filename_chars": ["<", ">", ":", "\"", "/", "\\", "|", "?", "*", "\u0000"],
    "enable_magic_number_validation": true,
    "enable_path_traversal_protection": true,
    "enable_file_quarantine": true,
    "quarantine_directory": "/tmp/proxemic_quarantine",
    "audit_enabled": true,
    "rate_limit_enabled": true,
    "max_requests_per_minute": 60
  },
  "logging": {
    "level": "warn",
    "format": "json",
    "file_enabled": true,
    "file_path": "./logs/proxemic.log",
    "console_enabled": true,
    "max_file_size_mb": 10,
    "max_files": 5,
    "structured_logging": true
  },
  "database": {
    "url": "sqlite:./Proxemic.db",
    "max_connections": 5,
    "connection_timeout_ms": 30000,
    "enable_migrations": true,
    "backup_enabled": true,
    "backup_interval_hours": 24
  },
  "ai": {
    "openrouter_api_key": null,
    "anthropic_api_key": null,
    "max_embedding_batch_size": 50,
    "vector_index_path": "./vector_index",
    "enable_cloud_sync": false,
    "enable_collaboration": false,
    "request_timeout_ms": 60000
  }
}
